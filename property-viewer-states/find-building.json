{
  "StartAt": "FindBuilding",
  "States": {
    "FindBuilding": {
      "Type": "Task",
      "InputPath": "$.uprn",
      "Resource": "module:findingById",
      "ResourceConfig": {
        "modelId": "building"
      },
      "ResultPath": "$.building",
      "Next": "OccupantsCheck"
    },
    "OccupantsCheck": {
      "Type": "Choice",
      "Choices": [
        {
          "Variable": "$.building.occupants",
          "IsUndefined": true,
          "Next": "FormatBuildingResults"
        },
        {
          "Variable": "$.building.occupants",
          "IsNull": true,
          "Next": "FormatBuildingResults"
        }
      ],
      "Default": "SetOccupancyRange"
    },
    "SetOccupancyRange": {
      "Type": "Task",
      "Resource": "module:runFunction",
      "ResourceConfig": {
        "functionName": "setOccupancyRange"
      },
      "Next": "FormatBuildingResults"
    },
    "FormatBuildingResults": {
      "Type": "Pass",
      "Parameters": {
        "key": "building",
        "value.$": "$.building"
      },
      "ResultPath": "$",
      "End": true
    }
  }
}
